  },
  "scripts": {
    "start": "concurrently \"npm run wds\" \"npm run server\"",
    "wds": "./node_modules/.bin/webpack-dev-server --open --hot",
    "server": "concurrently \"npm run tsc\" \"nodemon --watch server server/index.js\"",
    "tsc": "tsc --watch --p ./tsconfig.json",
    "build:dev": "rm -rf public/ && ./node_modules/.bin/webpack",
    "build:prod": "rm -rf public/ && ./node_modules/.bin/webpack --mode=production",
    "build:watch": "rm -rf public/ && ./node_modules/.bin/webpack -w",
    "lint": "npm run lintServer; npm run lintCSS",
    "lintCSS": "node_modules/.bin/stylelint --fix src/**/*",
    "lintServer": "node_modules/.bin/eslint --fix server/**.ts",
    "postinstall": "tsc",
    "docker": "docker-compose up"

  }
